<form id="new-config" action="{{site.uri.public}}/api/ufexcel/config" method="post">
  {% include "forms/csrf.html.twig" %}
  <div class="form-group has-feedback">
    <label>Table ID</label>
    <input type="text" class="form-control" name="table-id">
  </div>
  <div class="form-group has-feedback">
    <label>Database Table</label>
    <input type="text" class="form-control" name="db-table">
  </div>
  <div class="row">
    <div class="col-xs-8">
    </div>
    <div class="col-xs-6">
      <button type="submit" class="btn btn-primary btn-block btn-flat">Create</button>
    </div>
    <div class="col-xs-4">
      <button type="button" class="btn btn-block btn-lg btn-link" data-dismiss="modal">{{translate("CANCEL")}}</button>
    </div>
  </div>
</form>
